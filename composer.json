{
    "name": "stevebauman/inventory",
    "description": "Inventory management using Laravel 4 / 5",
    "keywords": ["laravel", "inventory", "inventory-management"],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman_7@hotmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "4.* | 5.*",
        "illuminate/support": "4.* | 5.*",
        "baum/baum": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "suggest": {
        "venturecraft/revisionable": "Allows the tracking of inventory stock location and category changes"
    },
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Inventory\\": "src/Stevebauman/Inventory/"
        }
    },
    "autoload-dev":
    {
        "psr-4": {
          "Stevebauman\\Inventory\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
